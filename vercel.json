{
    "version": 1,
    "builds": [
      {
        "src": "client/",
        "use": "@vercel/static-build"
      },
      {
        "src": "server/",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "server/index.js" },
      { "src": "/(.*)", "dest": "client/$1" }
    ]
  }